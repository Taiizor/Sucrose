<ResourceDictionary
     xmlns:system="clr-namespace:System;assembly=mscorlib"
     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
     xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
>
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="FullLibraryPage/Portal.FullLibraryPage.ES.xaml" />
        <ResourceDictionary Source="EmptyLibraryPage/Portal.EmptyLibraryPage.ES.xaml" />
        <ResourceDictionary Source="SearchLibraryPage/Portal.SearchLibraryPage.ES.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <system:String x:Key="Portal.LibraryPage.Title">LibraryPage</system:String>

    <system:String x:Key="Portal.LibraryPage.Import.Successful">El tema '{0}' se ha añadido correctamente a la biblioteca.</system:String>
    <system:String x:Key="Portal.LibraryPage.Import.Unsuccessful">No se ha podido añadir el tema '{0}' a la biblioteca. Motivo: '{1}'.</system:String>
</ResourceDictionary>