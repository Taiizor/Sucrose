<ResourceDictionary
     xmlns:system="clr-namespace:System;assembly=mscorlib"
     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
     xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
>
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="FullLibraryPage/Portal.FullLibraryPage.DE.xaml" />
        <ResourceDictionary Source="EmptyLibraryPage/Portal.EmptyLibraryPage.DE.xaml" />
        <ResourceDictionary Source="SearchLibraryPage/Portal.SearchLibraryPage.DE.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <system:String x:Key="Portal.LibraryPage.Title">LibraryPage</system:String>

    <system:String x:Key="Portal.LibraryPage.Import.Successful">Das Thema '{0}' wurde erfolgreich in die Bibliothek aufgenommen.</system:String>
    <system:String x:Key="Portal.LibraryPage.Import.Unsuccessful">Das Thema '{0}' konnte nicht zur Bibliothek hinzugefügt werden. Grund: '{1}'.</system:String>
</ResourceDictionary>