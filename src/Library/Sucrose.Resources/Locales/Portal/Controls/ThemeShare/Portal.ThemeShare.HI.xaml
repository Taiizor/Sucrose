<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.ThemeShare.Title">थीम साझा करें</system:String>
    <system:String x:Key="Portal.ThemeShare.Close">बंद करें</system:String>

    <system:String x:Key="Portal.ThemeShare.ThemeTitle">शीर्षक</system:String>
    <system:String x:Key="Portal.ThemeShare.ThemeDescription">विवरण</system:String>

    <system:String x:Key="Portal.ThemeShare.ThemeExport">निर्यात करें</system:String>
    <system:String x:Key="Portal.ThemeShare.ThemePublish">स्टोर में प्रकाशित करें</system:String>
    <system:String x:Key="Portal.ThemeShare.ThemeCopy">लिंक कॉपी करें</system:String>

    <system:String x:Key="Portal.ThemeShare.ThemePublish.Publish">प्रकाशित करें</system:String>
    <system:String x:Key="Portal.ThemeShare.ThemePublish.Preparing">प्रकाशन के लिए तैयारी जल्द ही शुरू होगी..</system:String>

    <system:String x:Key="Portal.ThemeShare.ThemePublish.Limit">आपकी दैनिक थीम अपलोड सीमा की जांच की जा रही है..</system:String>

    <system:String x:Key="Portal.ThemeShare.ThemePublish.Limit.Exceeded">आपने अपनी दैनिक थीम अपलोड सीमा को पूरा कर लिया है, कृपया बाद में पुनः प्रयास करें।</system:String>
    <system:String x:Key="Portal.ThemeShare.ThemePublish.Limit.Error">आपकी दैनिक थीम अपलोड सीमा की जांच करते समय अज्ञात त्रुटि हुई।</system:String>

    <system:String x:Key="Portal.ThemeShare.ThemePublish.Compress">अपलोड करने से पहले थीम को संकुचित किया जा रहा है..</system:String>
    <system:String x:Key="Portal.ThemeShare.ThemePublish.Compress.Error">थीम को संकुचित करते समय एक त्रुटि हुई।</system:String>

    <system:String x:Key="Portal.ThemeShare.ThemePublish.Size">संकुचित थीम का आकार जांचा जा रहा है..</system:String>

    <system:String x:Key="Portal.ThemeShare.ThemePublish.Size.Exceeded">संकुचित थीम को अपलोड नहीं किया जा सकता क्योंकि यह {0} MB से अधिक है।</system:String>

    <system:String x:Key="Portal.ThemeShare.ThemePublish.Check">अपलोड करने से पहले संपीड़ित थीम की अखंडता की जाँच की जा रही है..</system:String>
    <system:String x:Key="Portal.ThemeShare.ThemePublish.Check.Error">संपीड़ित थीम की अखंडता की जाँच करते समय एक त्रुटि हुई।</system:String>

    <system:String x:Key="Portal.ThemeShare.ThemePublish.Upload">संकुचित थीम को अपलोड के लिए तैयार किया जा रहा है..</system:String>

    <system:String x:Key="Portal.ThemeShare.ThemePublish.Upload.Start">संकुचित थीम अपलोड की जा रही है..</system:String>
    <system:String x:Key="Portal.ThemeShare.ThemePublish.Upload.Error">संकुचित थीम अपलोड करते समय अज्ञात त्रुटि हुई।</system:String>
    <system:String x:Key="Portal.ThemeShare.ThemePublish.Upload.Errored">संकुचित थीम अपलोड करते समय अप्रत्याशित त्रुटि हुई: {0}।</system:String>
    <system:String x:Key="Portal.ThemeShare.ThemePublish.Upload.Succeded">संकुचित थीम को सफलतापूर्वक अपलोड कर दिया गया है और सत्यापन के बाद स्टोर में प्रकाशित किया जाएगा।</system:String>

    <system:String x:Key="Portal.ThemeShare.SaveDialogFilter">ZIP फ़ाइलें (*.zip)|*.zip|सभी फ़ाइलें (*.*)|*.*</system:String>
    <system:String x:Key="Portal.ThemeShare.SaveDialogTitle">थीम फ़ाइल सहेजें</system:String>

    <system:String x:Key="Portal.ThemeShare.ThemePublish.Already.Close">समझ गया</system:String>
    <system:String x:Key="Portal.ThemeShare.ThemePublish.Already.Title">प्रकाशन चेतावनी</system:String>
    <system:String x:Key="Portal.ThemeShare.ThemePublish.Already.Message">चयनित थीम पहले से ही स्टोर से डाउनलोड की जा चुकी है।</system:String>
</ResourceDictionary>