<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.OtherSettingPage.Title">OtherSettingPage</system:String>

    <system:String x:Key="Portal.OtherSettingPage.ReportData">Laporan Kesalahan Otomatis</system:String>
    <system:String x:Key="Portal.OtherSettingPage.ReportData.Description">Sucrose mengambil tindakan untuk melaporkannya dengan cepat saat menemui kesalahan.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.StatisticsData">Bagikan Statistik Penggunaan</system:String>
    <system:String x:Key="Portal.OtherSettingPage.StatisticsData.Description">Sucrose memungkinkan pengumpulan statistik penggunaan Anda untuk berkontribusi pada pengembangan aplikasi.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.DiscordHook">Discord</system:String>
    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.Description">Sucrose menyediakan komunikasi dengan Discord.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.DiscordRefresh">Pembaruan Koneksi:</system:String>
    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.DiscordDelay">Waktu Pembaruan (Detik):</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Agent">Agen Internet</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Agent.Description">Identitas agen Anda dalam situasi yang memerlukan koneksi internet.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key">Token Akses Pribadi</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Key.Description">Diperlukan untuk menggunakan toko dan pembaruan aplikasi tanpa masalah.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key.HintKey">Klik di sini jika Anda tidak tahu cara mendapatkan token akses pribadi.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key.PersonalKey">Silakan masukkan token akses pribadi</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Key.PersonalKey.Valid">Silakan masukkan token akses pribadi yang valid</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Server">Server Pembaruan</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Server.Description">Server tempat pembaruan akan dilakukan.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Server.ServerHint">Catatan: Jika {0} dipilih sebagai server, Anda harus memasukkan {2} untuk menghindari batas permintaan. Jika tidak, Anda mungkin mengalami masalah dengan pembaruan. Jika {1} dipilih, Anda akan mendapatkan pengalaman pembaruan yang lebih lancar.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Channel">Saluran Pembaruan</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Channel.Description">Saluran yang digunakan untuk melakukan pembaruan.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Update">Metode Pembaruan</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Update.Description">Metode yang digunakan untuk melakukan pembaruan.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Update.Limit">Batas Lebar Pita Unduhan:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Auto">Pembaruan Otomatis</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Auto.Description">Apakah proses pembaruan akan diperiksa secara otomatis.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer">Alat Pengembang</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Developer.Description">Mengelola alat pengembang untuk mesin WebView dan CefSharp.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperMode">Alat Pengembang Default:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperPort">Port Debugging:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperHint">Petunjuk: Alat pengembang default mungkin tidak berfungsi dengan baik di tema yang didukung. Port debugging cenderung lebih baik. Ketika Anda siap, Anda dapat menggunakan alamat 'http://localhost:{0}' atau 'chrome://inspect' untuk browser Chrome, 'opera://inspect' untuk browser Opera, 'edge://inspect' untuk browser Edge.</system:String>
</ResourceDictionary>