<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.OtherSettingPage.Title">OtherSettingPage</system:String>

    <system:String x:Key="Portal.OtherSettingPage.ReportData">Otomatik Hata Raporlama</system:String>
    <system:String x:Key="Portal.OtherSettingPage.ReportData.Description">Sucrose, bir hatayla karşılaştığında bunu hızlıca bildirmek için harekete geçer.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.StatisticsData">Kullanım İstatistiklerini Paylaş</system:String>
    <system:String x:Key="Portal.OtherSettingPage.StatisticsData.Description">Sucrose, kullanım istatistiklerinizi toplayarak uygulamanın gelişmesine olanak sağlar.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.DiscordHook">Discord</system:String>
    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.Description">Sucrose, Discord ile arasındaki haberleşmeyi sağlar.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.DiscordRefresh">Bağlantı Yenileme:</system:String>
    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.DiscordDelay">Yenileme Süresi (Saniye):</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Agent">İnternet Temsilcisi</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Agent.Description">İnternet bağlantısı gerektiren durumlardaki temsilci kimliğiniz.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key">Kişisel Erişim Belirteci</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Key.Description">Mağazayı ve uygulama güncellemeyi sorunsuz bir şekilde kullanmak için gerekli.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key.HintKey">Kişisel erişim belirtecini nasıl elde edeceğinizi bilmiyorsanız buraya tıklayın.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key.PersonalKey">Lütfen bir kişisel erişim belirteci girin</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Key.PersonalKey.Valid">Lütfen geçerli bir kişisel erişim belirteci girin</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Server">Güncelleme Sunucusu</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Server.Description">Güncelleme işlemlerinin hangi sunucu üzerinden yapılacağı.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Server.ServerHint">Not: Sunucu olarak {0} seçilmişse istek limitlerine takılmamak için bir {2} girilmesi gerekiyor. Aksi taktirde güncelleme işlemlerinde sorun yaşayabilirsiniz. Eğer ki {1} seçilmişse görece daha sorunsuz bir güncelleme deneyimi yaşarsınız.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Module">Güncelleme Modülü</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Module.Description">Güncelleme işleminin hangi modül üzerinden yapılacağı.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Channel">Güncelleme Kanalı</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Channel.Description">Güncelleme işleminin hangi kanal üzerinden yapılıp yapılmayacağı.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Update">Güncelleme Yöntemi</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Update.Description">Güncelleme işleminin hangi yöntemle yapılıp yapılmayacağı.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Update.Limit">İndirme Bant Genişliği Sınırı:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Auto">Otomatik Güncelleme</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Auto.Description">Güncelleme işleminin otomatik olarak denetlenip denetlenmeyeceği.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer">Geliştirici Araçları</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Developer.Description">WebView ve CefSharp motoru için geliştirici araçlarını yönetin.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperMode">Varsayılan Geliştirici Araçları:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperPort">Hata Ayıklama Bağlantı Noktası:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperHint">İpucu: Varsayılan geliştirici araçları etkileşim desteklenen temalarda düzgün çalışmamaktadır. Hata ayıklama bağlantı noktası ise daha düzgün çalışmaktadır. Hazır olduğunuzda 'http://localhost:{0}' adresini veya Chrome tarayıcısı için 'chrome://inspect', Opera tarayıcısı için 'opera://inspect', Edge tarayıcısı için 'edge://inspect' adresini kullanabilirsiniz.</system:String>
</ResourceDictionary>