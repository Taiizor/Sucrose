<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.OtherSettingPage.Title">OtherSettingPage</system:String>

    <system:String x:Key="Portal.OtherSettingPage.ReportData">Automatisk Feilrapportering</system:String>
    <system:String x:Key="Portal.OtherSettingPage.ReportData.Description">Sucrose tar handling for å rapportere det raskt ved feil.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.StatisticsData">Del Bruksstatistikk</system:String>
    <system:String x:Key="Portal.OtherSettingPage.StatisticsData.Description">Sucrose muliggjør innsamling av bruksstatistikken din for å bidra til utviklingen av applikasjonen.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.DiscordHook">Discord</system:String>
    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.Description">Sucrose muliggjør kommunikasjon med Discord.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.DiscordRefresh">Oppdatering av tilkobling:</system:String>
    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.DiscordDelay">Oppdateringsfrekvens (sekunder):</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Agent">Nettverksagent</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Agent.Description">Din agentidentitet for situasjoner som krever nettverkstilgang.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key">Personlig tilgangstoken</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Key.Description">Nødvendig for å bruke butikken og applikasjonsoppdateringer problemfritt.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key.HintKey">Hvis du ikke vet hvordan du skal skaffe en personlig tilgangstoken, kan du klikke her.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key.PersonalKey">Vennligst angi en personlig tilgangstoken</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Key.PersonalKey.Valid">Vennligst angi en gyldig personlig tilgangstoken</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Server">Oppdateringsserver</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Server.Description">Serveren hvor oppdateringer vil bli utført.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Server.ServerHint">Merk: Hvis {0} er valgt som server, må du legge inn en {2} for å unngå forespørselsgrenser. Hvis ikke, kan du oppleve problemer med oppdateringer. Hvis {1} er valgt, vil du få en jevnere oppdateringsopplevelse.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Module">Oppdateringsmodul</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Module.Description">Hvilken modul som skal brukes til oppdateringen.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Channel">Oppdateringskanal</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Channel.Description">Kanalen som oppdateringsprosessen skal bruke eller ikke.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Update">Oppdateringsmetode</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Update.Description">Metoden som skal brukes for oppdateringsprosessen.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Update.Limit">Nedlastingsbåndbreddegrense:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Auto">Automatisk Oppdatering</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Auto.Description">Om oppdateringsprosessen vil bli sjekket automatisk.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer">Utviklerverktøy</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Developer.Description">Administrer utviklerverktøy for WebView og CefSharp-motoren.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperMode">Standard Utviklerverktøy:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperPort">Feilsøkingsport:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperHint">Hint: Standard utviklerverktøy fungerer kanskje ikke skikkelig i støttede temaer. Feilsøkingsporten pleier å fungere bedre. Når du er klar, kan du bruke 'http://localhost:{0}'-adressen eller 'chrome://inspect' for Chrome-nettleseren, 'opera://inspect' for Opera-nettleseren, 'edge://inspect' for Edge-nettleseren.</system:String>
