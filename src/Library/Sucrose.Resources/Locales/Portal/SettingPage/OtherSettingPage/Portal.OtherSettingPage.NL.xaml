<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.OtherSettingPage.Title">OtherSettingPage</system:String>

    <system:String x:Key="Portal.OtherSettingPage.ReportData">Automatische Foutenrapportage</system:String>
    <system:String x:Key="Portal.OtherSettingPage.ReportData.Description">Sucrose onderneemt actie om het snel te rapporteren bij een fout.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.StatisticsData">Deel Gebruiksstatistieken</system:String>
    <system:String x:Key="Portal.OtherSettingPage.StatisticsData.Description">Sucrose maakt het mogelijk om uw gebruiksstatistieken te verzamelen om bij te dragen aan de ontwikkeling van de applicatie.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.DiscordHook">Discord</system:String>
    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.Description">Sucrose maakt communicatie met Discord mogelijk.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.DiscordRefresh">Verbinding Vernieuwen:</system:String>
    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.DiscordDelay">Vernieuwingsinterval (seconden):</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Agent">Internetvertegenwoordiger</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Agent.Description">Uw vertegenwoordigers-ID in situaties waar internetverbinding vereist is.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key">Persoonlijke Toegangssleutel</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Key.Description">Noodzakelijk voor een naadloos gebruik van de winkel en app-updates.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key.HintKey">Als u niet weet hoe u een persoonlijke toegangssleutel kunt verkrijgen, klik dan hier.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key.PersonalKey">Voer alstublieft een persoonlijke toegangssleutel in</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Key.PersonalKey.Valid">Voer een geldige persoonlijke toegangssleutel in</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Server">Update Server</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Server.Description">De server waarop updates worden uitgevoerd.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Server.ServerHint">Opmerking: Als {0} is geselecteerd als server, moet u een {2} invoeren om aanvraaglimieten te vermijden. Anders kunt u problemen ondervinden bij updates. Als {1} is geselecteerd, zult u een soepeler update-ervaring hebben.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Module">Update-Module</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Module.Description">Welke module zal worden gebruikt voor de update.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Channel">Updatekanaal</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Channel.Description">Het kanaal waar de updatebewerking plaatsvindt.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Update">Update Methode</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Update.Description">De methode waarmee de updatebewerking wordt uitgevoerd.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Update.Limit">Downloadbandbreedtelimiet:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Auto">Automatische Update</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Auto.Description">Of het updateproces automatisch wordt gecontroleerd.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer">Ontwikkelaarshulpmiddelen</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Developer.Description">Beheer ontwikkelaarstools voor de WebView- en CefSharp-engine.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperMode">Standaard Ontwikkelaarstools:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperPort">Debugging-Poort:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperHint">Hint: Standaard ontwikkelaarstools werken mogelijk niet correct in ondersteunde thema's. De debugging-poort werkt over het algemeen beter. Wanneer je klaar bent, kun je het adres 'http://localhost:{0}' of 'chrome://inspect' voor de Chrome-browser, 'opera://inspect' voor de Opera-browser, 'edge://inspect' voor de Edge-browser gebruiken.</system:String>
</ResourceDictionary>