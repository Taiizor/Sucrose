<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.OtherSettingPage.Title">OtherSettingPage</system:String>

    <system:String x:Key="Portal.OtherSettingPage.ReportData">Raportare Automată a Erorilor</system:String>
    <system:String x:Key="Portal.OtherSettingPage.ReportData.Description">Sucrose ia măsuri pentru a raporta rapid când întâlnește o eroare.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.StatisticsData">Partajarea Statisticilor de Utilizare</system:String>
    <system:String x:Key="Portal.OtherSettingPage.StatisticsData.Description">Sucrose permite colectarea statisticilor dvs. de utilizare pentru a contribui la dezvoltarea aplicației.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.DiscordHook">Discord</system:String>
    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.Description">Sucrose facilitează comunicarea cu Discord.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.DiscordRefresh">Reîmprospătare Conexiune:</system:String>
    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.DiscordDelay">Interval de Reîmprospătare (secunde):</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Agent">Reprezentant Internet</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Agent.Description">Identitatea dvs. de reprezentant în situații care necesită conexiune la internet.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key">Cheie de Acces Personală</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Key.Description">Necesară pentru utilizarea fără probleme a magazinului și a actualizărilor aplicației.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key.HintKey">Faceți clic aici dacă nu știți cum să obțineți o cheie de acces personală.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key.PersonalKey">Vă rugăm să introduceți o cheie de acces personală</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Key.PersonalKey.Valid">Vă rugăm să introduceți o cheie de acces personală validă</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Server">Server de Actualizare</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Server.Description">Serverul prin care se vor face actualizările.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Server.ServerHint">Notă: Dacă {0} este selectat ca server, trebuie să introduceți un {2} pentru a evita limitele de cerere. În caz contrar, este posibil să întâmpinați probleme la actualizări. Dacă {1} este selectat, veți avea o experiență de actualizare mai lină.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Channel">Canal de Actualizare</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Channel.Description">Unde ar trebui să aibă loc procesul de actualizare.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Update">Metoda de Actualizare</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Update.Description">Cum ar trebui să se desfășoare procesul de actualizare.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Update.Limit">Limită Lățime de Bandă pentru Descărcare:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Auto">Actualizare Automată</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Auto.Description">Dacă procesul de actualizare va fi verificat automat.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer">Instrumente de Dezvoltare</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Developer.Description">Gestionați instrumentele de dezvoltare pentru motorul WebView și CefSharp.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperMode">Instrumente de Dezvoltare Implicite:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperPort">Port de Depanare:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperHint">Sugestie: Instrumentele de dezvoltare implicite pot să nu funcționeze corect în temele suportate. Portul de depanare are tendința să funcționeze mai bine. Când sunteți gata, puteți utiliza adresa 'http://localhost:{0}' sau 'chrome://inspect' pentru browserul Chrome, 'opera://inspect' pentru browserul Opera, 'edge://inspect' pentru browserul Edge.</system:String>
</ResourceDictionary>