<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.OtherSettingPage.Title">OtherSettingPage</system:String>

    <system:String x:Key="Portal.OtherSettingPage.ReportData">Segnalazione Automatica Degli Errori</system:String>
    <system:String x:Key="Portal.OtherSettingPage.ReportData.Description">Sucrose interviene per segnalarlo rapidamente in caso di errore.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.StatisticsData">Condividi Statistiche di Utilizzo</system:String>
    <system:String x:Key="Portal.OtherSettingPage.StatisticsData.Description">Sucrose consente la raccolta delle tue statistiche di utilizzo per contribuire allo sviluppo dell'applicazione.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.DiscordHook">Discord</system:String>
    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.Description">Sucrose facilita la comunicazione con Discord.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.DiscordRefresh">Aggiornamento Connessione:</system:String>
    <system:String x:Key="Portal.OtherSettingPage.DiscordHook.DiscordDelay">Intervallo di Aggiornamento (secondi):</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Agent">Rappresentante di Rete</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Agent.Description">La tua identità di rappresentante di rete per situazioni che richiedono una connessione a Internet.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key">Token di Accesso Personale</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Key.Description">Necessario per l'utilizzo senza problemi del negozio e degli aggiornamenti dell'applicazione.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key.HintKey">Fai clic qui se non sai come ottenere il token di accesso personale.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Key.PersonalKey">Inserisci un token di accesso personale</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Key.PersonalKey.Valid">Inserisci un token di accesso personale valido</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Server">Server di Aggiornamento</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Server.Description">Il server attraverso il quale verranno eseguiti gli aggiornamenti.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Server.ServerHint">Nota: Se viene selezionato {0} come server, è necessario inserire un {2} per evitare limiti di richieste. Altrimenti, potresti riscontrare problemi con gli aggiornamenti. Se viene selezionato {1}, avrai un'esperienza di aggiornamento più fluida.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Channel">Canale di Aggiornamento</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Channel.Description">Indica il canale tramite il quale verranno effettuati gli aggiornamenti.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Update">Metodo di Aggiornamento</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Update.Description">Indica il metodo di aggiornamento da utilizzare.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Update.Limit">Limite di Larghezza di Banda per il Download:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Auto">Aggiornamento Automatico</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Auto.Description">Se il processo di aggiornamento verrà controllato automaticamente.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer">Strumenti di Sviluppo</system:String>
    <system:String x:Key="Portal.OtherSettingPage.Developer.Description">Gestisci gli strumenti di sviluppo per il motore WebView e CefSharp.</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperMode">Strumenti di Sviluppo Predefiniti:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperPort">Porta di Debug:</system:String>

    <system:String x:Key="Portal.OtherSettingPage.Developer.DeveloperHint">Suggerimento: Gli strumenti di sviluppo predefiniti potrebbero non funzionare correttamente nei temi supportati. La porta di debug tende a funzionare meglio. Quando sei pronto, puoi utilizzare l'indirizzo 'http://localhost:{0}' o 'chrome://inspect' per il browser Chrome, 'opera://inspect' per il browser Opera, 'edge://inspect' per il browser Edge.</system:String>
</ResourceDictionary>