<ResourceDictionary
     xmlns:system="clr-namespace:System;assembly=mscorlib"
     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
     xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
>
    <system:String x:Key="Portal.PersonalSettingPage.Title">PersonalSettingPage</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Duration">Actualizare Magazin</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Duration.Description">Durata pentru care temele listate în magazin se vor actualiza.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.StoreStart">Pornire Automată</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.StoreStart.Description">Dacă temele descărcate din magazin vor fi setate automat ca fundal de ecran.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adult">Conținut pentru Adulți</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Adult.Description">Dacă se vor afișa sau nu conținuturile marcate ca fiind pentru adulți (NSFW) din magazin.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Confirm">Confirmare Ștergere</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Confirm.Description">Dacă se va cere confirmare când se șterg temele din bibliotecă.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Delete">Teme Corupte</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Delete.Description">Dacă temele corupte sau incomplete din bibliotecă vor fi șterse automat sau nu.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Delete.DeleteHint">Notă: Trebuie să fiți atent, deoarece aceasta poate duce la pierderea datelor.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.LibraryStart">Pornire Automată</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.LibraryStart.Description">Dacă temele create vor fi setate automat ca fundal de ecran.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Preview">Previzualizare Temă</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Preview.Description">Dacă temele vor fi sau nu previzualizate când treceti peste ele.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Preview.StorePreview">Previzualizare Magazin:</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Preview.StorePreviewHide">Ascunde Detaliile:</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Preview.LibraryPreview">Previzualizare Bibliotecă:</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Preview.LibraryPreviewHide">Ascunde Detaliile:</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Theme">Personalizare Temă</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Theme.Description">Personalizați numele sau descrierea temelor afișate.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Theme.TitleLength">Lungime Titlu (Caractere):</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Theme.DescriptionLength">Lungime Descriere (Caractere):</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adaptive">Aspect Adaptabil</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Adaptive.Description">Personalizați aspectul sau distanța dintre temele afișate.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adaptive.AdaptiveMargin">Margine (Pixeli):</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adaptive.AdaptiveLayout">Aspect Ordine (Bucăți):</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Store">Paginare Magazin</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Store.Description">Setați numărul de teme pentru fiecare pagină din magazin.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Library">Paginare Bibliotecă</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Library.Description">Setați numărul de teme pentru fiecare pagină din bibliotecă.</system:String>
</ResourceDictionary>