<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Portal.PersonalSettingPage.Title">PersonalSettingPage</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Server">Mağaza Sunucusu</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Server.Description">Mağazada listelenen temaların hangi sunucu üzerinden görüntüleneceği.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Server.ServerHint">Not: Sunucu olarak {0} seçilmişse istek limitlerine takılmamak için bir {2} girilmesi gerekiyor. Aksi taktirde mağazadaki temalara erişirken sorun yaşayabilirsiniz. Eğer ki {1} seçilmişse görece daha sorunsuz bir mağaza deneyimi yaşarsınız.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Duration">Mağaza Yenileme</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Duration.Description">Mağazada listelenen temaların ne kadar süreyle yenilenip yenilenmeyeceği.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.StoreStart">Otomatik Başlatma</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.StoreStart.Description">Mağazadan indirilen temanın otomatik duvar kağıdı olarak ayarlanıp ayarlanmayacağı.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adult">Güvenli Olmayan İçerikler</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Adult.Description">Mağazada güvenli değil (NSFW) olarak işaretlenmiş içeriklerin gösterilip gösterilmeyeceği.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Confirm">Silme Onayı</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Confirm.Description">Kütüphanedeki temaları silerken onay isteyip istemeyeceği.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Delete">Bozuk Temalar</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Delete.Description">Kütüphanedeki bozuk ya da eksik temaların otomatik silinip silinmeyeceği.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Delete.DeleteHint">Not: Dikkatli olunması gerekir. Aksi taktirde veri kayıplarına yol açabilir</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.LibraryStart">Otomatik Başlatma</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.LibraryStart.Description">Oluşturulan temanın otomatik duvar kağıdı olarak ayarlanıp ayarlanmayacağı.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Preview">Tema Önizleme</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Preview.Description">Görüntülenen temaların üzerine gelince önizlenip önizlenmeyeceği.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Preview.StorePreview">Mağaza Önizlemesi:</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Preview.StorePreviewHide">Detayların Gizlenmesi:</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Preview.LibraryPreview">Kütüphane Önizlemesi:</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Preview.LibraryPreviewHide">Detayların Gizlenmesi:</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adaptive">Uyarlanabilir Düzen</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Adaptive.Description">Görüntülenen temaların düzenini veya mesafesini özelleştirin.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adaptive.AdaptiveMargin">Boşluk Mesafesi (Piksel):</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Adaptive.AdaptiveLayout">Düzen Sırası (Adet):</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Sort">Kütüphane Sıralama</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Sort.Description">Kütüphanenizdeki temaların hangi biçimde ve nasıl sıralanacağını belirleyin.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Sort.SortMode">Sıralama Modu:</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Sort.SortKind">Sıralama Türü:</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Store">Mağaza Sayfalama</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Store.Description">Mağazadaki her bir sayfa için kaç tane tema olacağını belirleyin.</system:String>

    <system:String x:Key="Portal.PersonalSettingPage.Library">Kütüphane Sayfalama</system:String>
    <system:String x:Key="Portal.PersonalSettingPage.Library.Description">Kütüphanenizdeki her bir sayfa için kaç tane tema olacağını belirleyin.</system:String>
</ResourceDictionary>