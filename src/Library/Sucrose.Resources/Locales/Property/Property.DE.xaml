<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="Property.WindowTitle">Sucrose Property</system:String>

    <system:String x:Key="Property.DeleteTip">Kopie löschen</system:String>
    <system:String x:Key="Property.RefreshTip">Oberfläche aktualisieren</system:String>
    <system:String x:Key="Property.RestoreTip">Standard wiederherstellen</system:String>

    <system:String x:Key="Property.Type.NotSupport">Steuerelementtyp '{0}' wird nicht unterstützt.</system:String>

    <system:String x:Key="Property.ColorPicker.Color">Farbe</system:String>
    <system:String x:Key="Property.ColorPicker.Cancel">Abbrechen</system:String>
    <system:String x:Key="Property.ColorPicker.Confirm">Bestätigen</system:String>
    <system:String x:Key="Property.ColorPicker.EyeDropper">Pipette</system:String>
    <system:String x:Key="Property.ColorPicker.Popup.Title">Sucrose Property Color Picker</system:String>

    <system:String x:Key="Property.FileDropDown.Tip">Datei auswählen</system:String>
    <system:String x:Key="Property.FileDropDown.Filter">Alle Dateien (*.*)|*.*</system:String>

    <system:String x:Key="Property.FileDropDown.Access.Close">Verstanden</system:String>
    <system:String x:Key="Property.FileDropDown.Access.Title">Zugriffswarnung</system:String>
    <system:String x:Key="Property.FileDropDown.Access.Message">Sie haben keine Berechtigung, auf die ausgewählte Datei zuzugreifen.</system:String>
</ResourceDictionary>